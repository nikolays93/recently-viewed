{
  "autoload": {
    "psr-4": {
      "NikolayS93\\PluginName\\": "include\\class"
    }
  },
  "require": {
    "nikolays93/wp-admin-form": "dev-master",
    "nikolays93/wp-admin-page": "dev-master",
    "nikolays93/wp-admin-table": "dev-master"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/nikolays93/wp-admin-form"
    },
    {
      "type": "git",
      "url": "https://github.com/nikolays93/wp-admin-page"
    },
    {
      "type": "git",
      "url": "https://github.com/nikolays93/wp-admin-table"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "*",
    "squizlabs/php_codesniffer": "*",
    "wp-coding-standards/wpcs": "^0.13.1",
    "phpcompatibility/phpcompatibility-wp": "*"
  },
  "scripts": {
    "post-install-cmd": [
      "\"vendor/bin/phpcs\" --config-set installed_paths vendor/wp-coding-standards/wpcs"
    ],
    "post-update-cmd": [
      "\"vendor/bin/phpcs\" --config-set installed_paths vendor/wp-coding-standards/wpcs"
    ]
  }
}
